@model CardList
<div class="container-fluid mx-0 px-0 d-flex justify-content-between">
    <div class="justify-self-start">
        <h3>@Model.Name</h3>
        <span>@Model.Description<span>
    </div>
    <div class="d-flex">
        <button class="btn self-align-end">
            <img src="/images/pen-solid.svg" alt="Icon to update data" width="35px">
        </button>
        <button class="btn">
            <img src="/images/dumpster-solid.svg" alt="Icon to delete data" width="35px">
        </button>
    </div>
</div>